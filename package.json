{
  "name": "graphql-scalars",
  "version": "0.1.4 ",
  "description": "Custom scalars for GraphQL",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "clean": "rimraf dist",
    "build": "babel src --ignore *.test.js --out-dir dist",
    "prepublish": "npm run lint && npm run test:coverage && npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adriano-di-giovanni/graphql-scalars.git"
  },
  "keywords": [
    "graphql",
    "type",
    "scalar",
    "date",
    "email",
    "url"
  ],
  "author": "Adriano Di Giovanni <me@adrianodigiovanni.com> (http://adrianodigiovanni.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adriano-di-giovanni/graphql-scalars/issues"
  },
  "homepage": "https://github.com/adriano-di-giovanni/graphql-scalars#readme",
  "peerDependencies": {
    "graphql": "^0.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-jest": "^17.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "eslint": "^3.9.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.3.1",
    "eslint-plugin-standard": "^2.0.1",
    "graphql": "^0.7.2",
    "jest": "^17.0.0"
  },
  "jest": {
    "testPathDirs": [
      "src"
    ]
  },
  "files": [
    "dist"
  ]
